{
  "name": "modular-css",
  "version": "0.0.0",
  "private": true,
  "author": "Pat Cavit <npm@patcavit.com>",
  "homepage": "https://m-css.com",
  "license": "MIT",
  "repository": "tivac/modular-css",
  "bugs": {
    "url": "https://github.com/tivac/modular-css/issues"
  },
  "scripts": {
    "cover": "jest --coverage",
    "lint": "eslint .",
    "outdated": "lerna exec -- node ../../lerna/outdated.js",
    "parsers": "node parsers/build.js",
    "release": "env-cmd lerna publish",
    "prepublishOnly": "npm run parsers",
    "pretest": "npm run parsers",
    "test": "jest",
    "watch": "jest --watch",
    "www:build": "lerna run --scope @modular-css/website --stream build",
    "www:start": "lerna run --scope @modular-css/website --stream start",
    "prewww:deploy": "npm run parsers",
    "www:deploy": "lerna run --scope @modular-css/website --stream deploy"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@modular-css/test-utils": "file:packages/test-utils",
    "@modular-css/website": "file:packages/www",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@tivac/eslint-config": "^2.4.0",
    "browserify": "^16.2.3",
    "cli-tester": "2.0.0",
    "cssnano": "^5.0.11",
    "dedent": "0.7.0",
    "dentist": "1.0.3",
    "env-cmd": "^10.0.1",
    "eslint": "^8.2.0",
    "eslint-plugin-jest": "^25.2.4",
    "factor-bundle": "2.5.0",
    "from2-string": "1.1.0",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "lerna": "^4.0.0",
    "node-notifier": "^10.0.0",
    "p-defer": "^3.0.0",
    "peggy": "^1.2.0",
    "postcss": "^8.3.11",
    "read-dir-deep": "^7.0.1",
    "rollup": "^2.60.0",
    "rollup-plugin-hypothetical": "^2.1.1",
    "rollup-plugin-svelte": "^7.1.0",
    "shelljs": "^0.8.4",
    "sirv": "^0.4.2",
    "snapshot-diff": "^0.9.0",
    "sugarss": "^4.0.1",
    "svelte": "^3.44.1",
    "watchify": "^3.11.1",
    "webpack": "^4.30.0"
  },
  "dependencies": {
    "@modular-css/browserify": "file:packages/browserify",
    "@modular-css/cli": "file:packages/cli",
    "@modular-css/glob": "file:packages/glob",
    "@modular-css/path-aliases": "file:packages/aliases",
    "@modular-css/path-resolver": "file:packages/paths",
    "@modular-css/postcss": "file:packages/postcss",
    "@modular-css/processor": "file:packages/processor",
    "@modular-css/rollup": "file:packages/rollup",
    "@modular-css/rollup-rewriter": "file:packages/rollup-rewriter",
    "@modular-css/shortnames": "file:packages/namer",
    "@modular-css/svelte": "file:packages/svelte",
    "@modular-css/webpack": "file:packages/webpack"
  }
}
