{
  "name": "modular-css",
  "version": "0.29.1",
  "description": "A curated subset of css-modules available as CLI, API, or Browserify transform",
  "main": "./index.js",
  "repository": "tivac/modular-css",
  "bugs": {
    "url": "https://github.com/tivac/modular-css/issues"
  },
  "bin": "./bin/cli.js",
  "scripts": {
    "cover": "istanbul cover --print both ./node_modules/mocha/bin/_mocha",
    "commitmsg": "validate-commit-msg",
    "lint": "eslint .",
    "parser": "pegjs -o ./src/parsers/parser.js ./src/parsers/parser.pegjs",
    "test": "mocha",
    "posttest": "npm run lint",
    "preversion": "npm parser && npm test",
    "version": "changes",
    "postversion": "git push --follow-tags",
    "watch": "mocha -w -R min"
  },
  "author": "Pat Cavit <npm@patcavit.com>",
  "license": "MIT",
  "keywords": [
    "browserify",
    "browserify-plugin",
    "browserify-transform",
    "css",
    "css-modules",
    "modular-css",
    "postcss"
  ],
  "devDependencies": {
    "@studio/changes": "^1.0.5",
    "browserify": "^13.0.0",
    "common-tags": "^1.4.0",
    "eslint": "^3.12.0",
    "eslint-config-arenanet": "^3.0.0",
    "factor-bundle": "^2.5.0",
    "from2-string": "^1.1.0",
    "husky": "^0.12.0",
    "istanbul": "^0.4.2",
    "mocha": "^3.0.0",
    "pegjs": "^0.10.0",
    "postcss-color-rebeccapurple": "^2.0.0",
    "rimraf": "^2.5.0",
    "rollup": "^0.39.2",
    "shelljs": "^0.7.3",
    "validate-commit-msg": "^2.8.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "dependency-graph": "^0.5.0",
    "escape-string-regexp": "^1.0.5",
    "esutils": "^2.0.2",
    "globule": "^1.0.0",
    "lodash.difference": "^4.0.0",
    "lodash.filter": "^4.2.1",
    "lodash.findlast": "^4.4.0",
    "lodash.foreach": "^4.0.0",
    "lodash.get": "^4.4.2",
    "lodash.invert": "^4.0.0",
    "lodash.mapvalues": "^4.4.0",
    "lodash.uniq": "^4.2.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "postcss": "^5.2.5",
    "postcss-selector-parser": "^2.0.0",
    "postcss-url": "^5.1.0",
    "postcss-value-parser": "^3.2.3",
    "promise-sequential": "^1.1.1",
    "resolve-from": "^2.0.0",
    "rollup-pluginutils": "^1.3.1",
    "sink-transform": "^2.0.0",
    "through2": "^2.0.0",
    "unique-slug": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "arenanet",
    "env": {
      "node": true,
      "mocha": true
    },
    "globals": {
      "Promise": true
    },
    "rules": {
      "prefer-template": "off"
    }
  }
}
