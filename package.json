{
  "name": "modular-css",
  "version": "0.0.0",
  "private": true,
  "author": "Pat Cavit <npm@patcavit.com>",
  "homepage": "https://m-css.com",
  "license": "MIT",
  "repository": "tivac/modular-css",
  "bugs": {
    "url": "https://github.com/tivac/modular-css/issues"
  },
  "scripts": {
    "cover": "jest --coverage",
    "outdated": "lerna exec --no-bail --stream -- npm outdated",
    "parser": "pegjs packages/processor/parsers/parser.pegjs",
    "release": "env-cmd .env lerna publish",
    "pretest": "npm run parser",
    "test": "jest",
    "watch": "jest --watch",
    "www:build": "lerna run --stream build packages/www",
    "www:start": "lerna run --stream start packages/www",
    "www:deploy": "cd ./packages/www && npm run production"
  },
  "devDependencies": {
    "@modular-css/test-utils": "file:packages/test-utils",
    "@modular-css/website": "file:packages/www",
    "@tivac/eslint-config": "^2.3.1",
    "browserify": "^16.2.3",
    "cli-tester": "2.0.0",
    "cssnano": "^4.1.10",
    "dedent": "0.7.0",
    "dentist": "1.0.3",
    "env-cmd": "^8.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "factor-bundle": "2.5.0",
    "from2-string": "1.1.0",
    "jest": "^24.7.0",
    "jest-cli": "^24.7.0",
    "jest-runner-eslint": "^0.7.3",
    "lerna": "^3.13.1",
    "pegjs": "0.10.0",
    "read-dir-deep": "^4.0.3",
    "rollup": "^1.7.4",
    "rollup-plugin-svelte": "^5.0.3",
    "shelljs": "^0.8.3",
    "snapshot-diff": "^0.5.1",
    "sugarss": "^2.0.0",
    "svelte": "^2.16.1",
    "watchify": "^3.11.1",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "@modular-css/browserify": "file:packages/browserify",
    "@modular-css/cli": "file:packages/cli",
    "@modular-css/glob": "file:packages/glob",
    "@modular-css/path-aliases": "file:packages/aliases",
    "@modular-css/path-resolver": "file:packages/paths",
    "@modular-css/postcss": "file:packages/postcss",
    "@modular-css/processor": "file:packages/processor",
    "@modular-css/rollup": "file:packages/rollup",
    "@modular-css/rollup-rewriter": "file:packages/rollup-rewriter",
    "@modular-css/shortnames": "file:packages/namer",
    "@modular-css/svelte": "file:packages/svelte",
    "@modular-css/webpack": "file:packages/webpack"
  }
}
