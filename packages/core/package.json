    "css-modules",
    "modular-css",
    "postcss"
  ],
  "devDependencies": {
    "@studio/changes": "^1.0.5",
    "dentist": "^1.0.3",
    "husky": "^0.13.1",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "pegjs": "^0.10.0",
    "shelljs": "^0.7.3",
    "validate-commit-msg": "^2.8.2"
  },
  "dependencies": {
    "dependency-graph": "^0.5.0",
    "escape-string-regexp": "^1.0.5",
    "lodash.findlast": "^4.4.0",
    "lodash.foreach": "^4.0.0",
    "lodash.get": "^4.4.2",
    "lodash.invert": "^4.0.0",
    "lodash.mapvalues": "^4.4.0",
