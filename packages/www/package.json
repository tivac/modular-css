{
  "name": "www",
  "version": "1.0.0",
  "private": true,
  "description": "web site for modular-css",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "publish": "surge",
    "build": "browserify -p [ modular-cssify --css \"./gen/style.css\" ] index.js -o gen/app.js",
    "watch": "watchify -p [ modular-cssify --css \"./gen/style.css\" ] index.js -o gen/app.js",
    "start": "beefy index.js -- -o gen/app.js"
  },
  "author": "Pat Cavit <npm@patcavit.com>",
  "license": "MIT",
  "dependencies": {
    "codemirror": "^5.24.2",
    "mithril": "^1.0.1",
    "modular-css-core": "^4.0.0",
    "prismjs": "^1.6.0"
  },
  "devDependencies": {
    "beefy": "^2.1.8",
    "live-server": "^1.2.0",
    "modular-css-rollup": "^4.0.0",
    "rollup": "^0.41.4",
    "modular-cssify": "^4.0.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "surge": "^0.18.0",
    "watchify": "^3.9.0"
  }
}
