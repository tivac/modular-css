{
  "name": "@modular-css/website",
  "version": "21.0.0",
  "private": true,
  "description": "web site for modular-css",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "predeploy": "npm run dist",
    "deploy": "surge --project ./gen --domain m-css.com",
    "dist": "webpack --env=dist --optimize-minimize --define process.env.NODE_ENV=\"'production'\"",
    "start": "webpack-dev-server",
    "test": "echo \"no tests ¯\\_(ツ)_/¯\""
  },
  "author": "Pat Cavit <npm@patcavit.com>",
  "license": "MIT",
  "dependencies": {
    "@modular-css/processor": "file:../processor",
    "clipboard": "1.7.1",
    "codemirror": "5.39.2",
    "memory-fs": "0.4.1",
    "mithril": "1.1.6",
    "path-parse": "1.0.6",
    "throttleit": "1.0.0"
  },
  "devDependencies": {
    "@modular-css/shortnames": "^21.0.0",
    "@modular-css/webpack": "^21.0.0",
    "buble-loader": "0.4.1",
    "copy-webpack-plugin": "4.5.2",
    "cssnano-cli": "1.0.5",
    "html-webpack-plugin": "2.30.1",
    "surge": "0.19.0",
    "webpack": "3.12.0",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-common-shake": "1.5.3",
    "webpack-dev-server": "2.11.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true
    }
  }
}
