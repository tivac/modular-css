{
    "name": "@modular-css/vscode",
    "description": "VS Code extension to provide custom CSS data",
    "version": "0.0.1",
    "publisher": "modular-css",
    "repository": "https://github.com/tivac/modular-css/packages/vscode",
    "engines": {
        "vscode": "^1.74.0"
    },
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.helloWorld",
                "title": "Hello World"
            }
        ],
        "css": {
            "customData": [
                "./data/m-css.css-data.json"
            ]
        }
    },
    "scripts": {},
    "devDependencies": {
        "@types/vscode": "^1.73.0"
    },
    "peerDependencies": {
        "@modular-css/processor": "^29"
    },
    "imports": {
        "#vscode": "./vscode.mjs"
    }
}